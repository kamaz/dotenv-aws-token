{
  "name": "dotenv-aws-mfa",
  "description": "Helps to maintain MFA token in dotenv file",
  "main": "dist/index.js",
  "bin": "dist/index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kamaz/dotenv-aws-mfa"
  },
  "bugs": {
    "url": "https://github.com/kamaz/dotenv-aws-mfa/issues"
  },
  "keywords": [
    "aws",
    "token",
    "mfa",
    "authentication",
    "dotenv",
    "nodejs",
    "profile"
  ],
  "scripts": {
    "commit": "git-cz",
    "build": "tsc",
    "watch": "tsc -w",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "prerelease": "git-authors-cli && git add .",
    "semantic-release": "semantic-release",
    "release": "standard-version -a",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "aws-sdk": "^2.604.0",
    "chalk": "^4.1.0",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "inquirer": "^7.0.3",
    "ramda": "^0.27.0",
    "yargs": "^15.1.0"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/ini": "^1.3.30",
    "@types/inquirer": "^6.5.0",
    "@types/jest": "^25.1.1",
    "@types/node": "^16.3.2",
    "@types/ramda": "^0.27.4",
    "@types/rc": "^1.1.0",
    "git-authors-cli": "^1.0.18",
    "git-cz": "^4.1.0",
    "jest": "^25.1.0",
    "semantic-release": "^17.0.7",
    "standard-version": "^8.0.0",
    "ts-jest": "^26.1.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "contributors": [
    "Kamaz <kamaz@xsimple.org>"
  ],
  "version": "0.0.5-0"
}
